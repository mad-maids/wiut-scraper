{
  "$schema": "https://json.schemastore.org/swcrc",
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": true,
      "decorators": true,
      "dynamicImport": true
    },
    "target": "es2019",
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    },
  },
  "module": {
    "type": "commonjs"
  },
  "minify": false
}